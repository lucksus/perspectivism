{
  "name": "perspectivism",
  "version": "0.0.1",
  "license": "CAL-1.0",
  "main": "main.js",
  "scripts": {
    "build-languages": "for OUTPUT in $(ls src/languages); do echo \"Building: $OUTPUT\" && cd src/languages/$OUTPUT && npm i && npm run build && cd -; done",
    "build": "tsc && rollup -c && npm run build-languages",
    "dev": "tsc && rollup -c -w",
    "serve": "sirv public",
    "validate": "svelte-check",
    "electron": "electron .",
    "start": "electron .",
    "lint": "tslint -c tslint.json \"src/core/**/*.ts\"",
    "lint:fix": "tslint --fix -c tslint.json \"src/core/**/*.ts\"",
    "test": "tsc && jest",
    "rebuild": "electron-rebuild -f -w serialport",
    "build-linux": "electron-packager . Perspectivism --platform=linux --overwrite --icon=./graphics/Logo.png"
  },
  "devDependencies": {
    "@apollo/client": "^3.2.5",
